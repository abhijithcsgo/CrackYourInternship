/**
 * @param {number[]} nums
 * @return {void} Do not return anything, modify nums in-place instead.
 */
var moveZeroes = function(nums) {
   var writeIndex = 0;
   var len = nums.length;
   for(var i = 0 ; i < len ; i++){
    if(nums[i] !== 0){
        nums[writeIndex] = nums[i];
        writeIndex++;
    }
   }
   for(var i = writeIndex ; i < len ; i++){
    nums[i] = 0;
   }
};